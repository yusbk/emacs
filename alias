# kopi fail alias ke ~/.emacs.d/eshell/ untuk mengaktifkan

alias gitw ~/Git-work/
alias gitp ~/Git-personal/
alias gitb cd ~/Git-personal/yusbk.github.io/org
alias gite cd ~/Git-personal/emacs/
alias cde cd ~/.emacs.d
alias cdd cd ~/Dropbox/
alias cdm cd ~/Dropbox/org/Meetings/
alias cpe2 cp emacs25.org ~/Git-personal/emacs/ ; cd ~/Git-personal/emacs
alias cpe1 cp emacs25.org ~/.emacs.d/ ; cd ~/.emacs.d
alias dd dired $1
alias la ls -a
alias ll ls -l
alias ff find-file $1
alias bkd find -name "#*#" -print -delete
alias ei encfs ~/Dropbox/.encrypted ~/avid
alias eo fusermount -u ~/avid
alias ef cd ~/avid
